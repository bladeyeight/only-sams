{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "bladeyeight/only-sams:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "80",
      "HostPort": "80"
    },
    {
      "ContainerPort": "443",
      "HostPort": "443"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/current/certs",
      "ContainerDirectory": "/certs"
    }
  ],
  "Logging": "/var/log/nginx"
}